
VAR[] Map
VAR MapWidth 15
VAR MapHeight 10
VAR MapSize
MUL MapSize MapWidth MapHeight

VAR IxLoop1;
 
InitMap:
	SET IxLoop1 0
InitMapLoop:
	PUSH Map 0
	INC IxLoop1
	CMP IxLoop1 MapSize
	JP!= InitMapLoop
	MSGB Map

ClearMap:
	SET IxLoop1 0
ClearMapLoop:
	SET [Map IxLoop1] -1
	INC IxLoop1
	CMP IxLoop1 MapSize
	JP!= ClearMapLoop
	MSGB Map
	
EXIT
