; This program will display the default color palette

PIXL 3 3
OUTM 1
OUTS "Default Palette" 0 0 10 0 0
NUM c 0
NUM x 0
NUM y 2
NUM key 0

loop:
	OUT 0 x y c c 1
	INC x
	OUT 0 x y c c 1
	INC c
	CMP c 256
	JP= end
	INC x
	CMP x 32
	JP!= loop
	INC y
	SET x 0
	JP loop

end:
	INP key
	CMP key $KB.ESC
	JP!= end
	RUN "Program Menu.ptml"
