; This program will display the default color palette

OUTS "Default Palette" 0 0 10 0 0
VAR c 0
VAR x 0
VAR y 2
VAR key 0

loop:
	OUT 0 x y c c 1
	INC x
	OUT 0 x y c c 1
	INC c
	CMP c 256
	JP= end
	INC x
	CMP x 32
	JP!= loop
	INC y
	SET x 0
	JP loop

end:
	INP key
	CMP key $KB.ESC
	JP!= end
	RUN "Program Menu.ptml"
